<form method="post" action="{{ path('admin_email_signature_delete', {'id': email_signature.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ email_signature.id) }}">
    <button class="button-danger">{{ 'app.default.delete'|trans }}</button>
</form>
