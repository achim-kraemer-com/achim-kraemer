<form method="post" action="{{ path('admin_time_entry_delete', {'id': time_entry.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ time_entry.id) }}">
    <button class="button-danger">{{ 'app.default.delete'|trans }}</button>
</form>
